{
  "name": "LinkedIn SalesNavigator mark as read",
  "description": "Mark all notifications in LinkedIn SalesNavigator as read",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon_16.png",
    "32": "icon_38.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": "linkedin-salesnavigator-chrome-extension-icon.png"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "permissions": ["scripting", "activeTab"]
}
